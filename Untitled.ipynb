   "source": [
    "import collections\n",
    "X = [4,3,3,9,4,3,2,1]\n",
    "Y = 2\n",
    "ans = None\n",
    "\n",
    "# initializing dict to store frequency of each element\n",
    "elements_count = collections.Counter(X)\n",
    "\n",
    "frequency = list(elements_count.values())\n",
    "frequency.sort(reverse=True)\n",
    "ans = len(X)-sum(frequency[:Y])\n",
    "print(\"Answer is:\",ans)"
   ]
